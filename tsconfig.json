{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ES2015"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": [
    "src"
  ],
  "paths": {
    "pages/*": [
      "pages/*"
    ],
    "containers/*": [
      "containers/*"
    ],
    "components/*": [
      "components/*"
    ],
    "sections/*": [
      "sections/*"
    ],
    "constants/*": [
      "constants/*"
    ],
    "models/*": [
      "models/*"
    ],
    "remotes/*": [
      "remotes/*"
    ],
    "styles/*": [
      "styles/*"
    ],
    "utils/*": [
      "utils/*"
    ],
    "hooks/*": [
      "hooks/*"
    ],
    "lib/*": [
      "lib/*"
    ],
    "stores/*": [
      "stores/*"
    ]
  }
}
